        tools:layout="@layout/fragment_cong_viec" >
        <action
            android:id="@+id/action_congViecFragment_to_hinhAnhCongViecFragment"
            app:destination="@id/hinhAnhCongViecFragment" />
    </fragment>
    <fragment
        android:id="@+id/hinhAnhCongViecFragment"
        android:name="com.example.kotlin_appquanlycongviec.fragment.HinhAnhCongViecFragment"
        android:label="fragment_hinh_anh_cong_viec"
        tools:layout="@layout/fragment_hinh_anh_cong_viec" />
    <fragment
        android:id="@+id/taiKhoanFragment"
        android:name="com.example.kotlin_appquanlycongviec.fragment.TaiKhoanFragment"
        android:label="fragment_tai_khoan"
        tools:layout="@layout/fragment_tai_khoan" />
    <fragment
        android:id="@+id/ghiChuFragment"
        android:name="com.example.kotlin_appquanlycongviec.fragment.GhiChuFragment"
