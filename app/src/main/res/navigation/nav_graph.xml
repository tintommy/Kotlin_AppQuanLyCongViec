    </fragment>
    <fragment
        android:id="@+id/taoVaXemGhiChuFragment"
        android:name="com.example.kotlin_appquanlycongviec.fragment.TaoVaXemGhiChuFragment"
        android:label="fragment_tao_va_xem_ghi_chu"
        tools:layout="@layout/fragment_tao_va_xem_ghi_chu" />
</navigation>