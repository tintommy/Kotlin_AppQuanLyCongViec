        <action
            android:id="@+id/action_congViecChuaLamFragment_to_hinhAnhCongViecFragment"
            app:destination="@id/hinhAnhCongViecFragment" />
    </fragment>
    <fragment
        android:id="@+id/taoMaPinGhiChuFragment"
        android:name="com.example.kotlin_appquanlycongviec.fragment.TaoMaPinGhiChuFragment"
